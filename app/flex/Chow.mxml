<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml"
  xmlns:generated="chow.components.generated.*"
  paddingBottom="8" paddingLeft="8" paddingRight="8" paddingTop="8"
  layout="horizontal" styleName="plain" initialize="init()">
  <mx:Script>
    <![CDATA[
      import org.restfulx.Rx;
      import chow.controllers.ApplicationController;

      private function init():void {
        ApplicationController.initialize();
      }
    ]]>
  </mx:Script>
  <mx:LinkBar dataProvider="{mainViewStack}" direction="vertical" borderStyle="solid" backgroundColor="#EEEEEE"/>
  <mx:ViewStack id="mainViewStack" width="100%" height="100%">
    <!-- For a simple demo, put all the components here. -->
    <generated:AssignedAssetBox/>
    <generated:AssignedSectionBox/>
    <generated:CachedPageBox/>
    <generated:ContentBox/>
    <generated:ContentVersionBox/>
    <generated:FeedbackBox/>
    <generated:HappeningBox/>
    <generated:MediumBox/>
    <generated:MembershipBox/>
    <generated:PaloozaPluginBox/>
    <generated:SectionBox/>
    <generated:SiteBox/>
    <generated:TagBox/>
    <generated:TaggingBox/>
  </mx:ViewStack>
</mx:Application>
